var hh = "";
if (user.username) {
  hh = "[@"+ user.username + "]";
}

function touchingOwnLink() {
  Bot.sendMessage("*âŒ Stop Clicking Your Own Link*");
}

function attractedByUser(refUser) {
  Api.sendMessage({
    chat_id: refUser.telegramid,
    text:
      "<b>â­ï¸ You Got a New </b><a href='tg://user?id=" +
